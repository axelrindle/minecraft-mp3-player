{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "MP3 Player mod",
  "description": "A minecraft mod that transforms the jukebox into a fully-fledged MP3 player.",
  "authors": [
    "lalo5",
    "trinzto"
  ],
  "contact": {
    "sources": "https://github.com/axelrindle/minecraft-mp3-player"
  },

  "license": "CC0-1.0",
  "icon": "assets/mp3_player/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.example.ExampleMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
